<?xml version="1.0" encoding="utf-8"?>
<unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
  <!-- Namespaces -->
  <namespace name="Contracts"/>
  <namespace name="DataContracts"/>
  <namespace name="Common"/>
  <namespace name="RegistrationManager"/>
  <namespace name="RegistrationEngine"/>
  <namespace name="DataAccessor" />

  <!-- Assemblies -->
  <assembly name="RegistrationEngine" />
  <assembly name="RegistrationManager" />
  <assembly name="Common" />
  <assembly name="DataAccessor" />
  <assembly name="Contracts" />


  <!-- Default Container -->
  <container>
    <type type="IRegistrationManager" mapTo="RegistrationManager"/>
    <type type="IRegistrationEngine" mapTo="RegistrationEngine"/>
    <type type="IRegistrationAccessor" mapTo="RegistrationAccessor"/>
  </container>
</unity>
